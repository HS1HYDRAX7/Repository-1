FPESTab:Toggle({
	Title="ESP Teachers",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState={} end
		local state=CallbackState
		state.highlights=state.highlights or {}
		state.conns=state.conns or {}

		local function clear()
			for _,h in ipairs(state.highlights) do
				if h then h:Destroy() end
			end
			state.highlights={}
			for _,c in ipairs(state.conns) do
				if c then c:Disconnect() end
			end
			state.conns={}
		end

		if not v then
			clear()
			return
		end

		local folder=workspace:FindFirstChild("Teachers")
		if not folder then return end

		for _,m in ipairs(folder:GetChildren()) do
			if m:IsA("Model") then
				local h=Instance.new("Highlight")
				h.Adornee=m
				h.FillColor=Color3.fromRGB(255,0,0)
				h.OutlineColor=Color3.fromRGB(255,0,0)
				h.FillTransparency=0.5
				h.Parent=m
				table.insert(state.highlights,h)
			end
		end

		table.insert(state.conns,folder.ChildAdded:Connect(function() clear(); if v then Callback(v) end end))
		table.insert(state.conns,folder.ChildRemoved:Connect(function() clear(); if v then Callback(v) end end))
	end
})

FPESTab:Toggle({
	Title="ESP Students",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState={} end
		local state=CallbackState
		state.highlights=state.highlights or {}
		state.conns=state.conns or {}

		local function clear()
			for _,h in ipairs(state.highlights) do
				if h then h:Destroy() end
			end
			state.highlights={}
			for _,c in ipairs(state.conns) do
				if c then c:Disconnect() end
			end
			state.conns={}
		end

		if not v then
			clear()
			return
		end

		local folder=workspace:FindFirstChild("Students")
		if not folder then return end

		for _,m in ipairs(folder:GetChildren()) do
			if m:IsA("Model") then
				local h=Instance.new("Highlight")
				h.Adornee=m
				h.FillColor=Color3.fromRGB(0,255,0)
				h.OutlineColor=Color3.fromRGB(0,255,0)
				h.FillTransparency=0.5
				h.Parent=m
				table.insert(state.highlights,h)
			end
		end

		table.insert(state.conns,folder.ChildAdded:Connect(function() clear(); if v then Callback(v) end end))
		table.insert(state.conns,folder.ChildRemoved:Connect(function() clear(); if v then Callback(v) end end))
	end
})

FPESTab:Toggle({
	Title="Proximity Alert",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState={} end
		local state=CallbackState
		if state.conn then state.conn:Disconnect() state.conn=nil end
		if not v then return end

		local Players=game:GetService("Players")
		local RunService=game:GetService("RunService")
		local lp=Players.LocalPlayer
		local last=0

		state.conn=RunService.Heartbeat:Connect(function()
			local char=lp.Character
			local hrp=char and char:FindFirstChild("HumanoidRootPart")
			if not hrp then return end
			local folder=workspace:FindFirstChild("Teachers")
			if not folder then return end
			for _,m in ipairs(folder:GetChildren()) do
				if m:IsA("Model") then
					local root=m:FindFirstChild("HumanoidRootPart")
					if root and (root.Position-hrp.Position).Magnitude<=15 then
						if os.clock()-last>=3 then
							last=os.clock()
							WindUI:Notify({
								Title="Proximity Alert",
								Content=m.Name.." is too close!",
								Duration=3,
								Icon="skull",
							})
						end
						break
					end
				end
			end
		end)
	end
})

