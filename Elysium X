OthersTab:Toggle({
Title="Third Person",
Value=false,
Callback=function(v)
	if v then
		if getgenv().ThirdPersonEnabled then return end
		getgenv().ThirdPersonEnabled = true

		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")

		local player = Players.LocalPlayer
		local camera = workspace.CurrentCamera
		local character = player.Character or player.CharacterAdded:Wait()
		local hrp = character:WaitForChild("HumanoidRootPart")

		player.CameraMode = Enum.CameraMode.Classic
		camera.CameraType = Enum.CameraType.Custom

		getgenv().ThirdPersonConn = RunService.RenderStepped:Connect(function()
			if not getgenv().ThirdPersonEnabled then return end
			camera.CFrame = CFrame.new(
				hrp.Position - hrp.CFrame.LookVector * 6 + Vector3.new(0, 2, 0),
				hrp.Position
			)
		end)
	else
		getgenv().ThirdPersonEnabled = false
		if getgenv().ThirdPersonConn then
			pcall(function() getgenv().ThirdPersonConn:Disconnect() end)
			getgenv().ThirdPersonConn = nil
		end
	end
end
})

OthersTab:Toggle({
Title="Shiftlock",
Value=false,
Callback=function(v)
	if getgenv().ShiftlockEnabled == nil then
		getgenv().ShiftlockEnabled = false
	end

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")

	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local hrp = character:WaitForChild("HumanoidRootPart")

	if not getgenv().ShiftlockGui then
		local gui = Instance.new("ScreenGui")
		gui.ResetOnSpawn = false
		gui.Parent = player:WaitForChild("PlayerGui")

		local img = Instance.new("ImageButton")
		img.Size = UDim2.fromOffset(48,48)
		img.Position = UDim2.new(0,10,0.5,-24)
		img.BackgroundTransparency = 1
		img.Image = "rbxassetid://80450981243325"
		img.Parent = gui

		getgenv().ShiftlockGui = gui
		getgenv().ShiftlockButton = img

		img.MouseButton1Click:Connect(function()
			getgenv().ShiftlockEnabled = not getgenv().ShiftlockEnabled
			img.Image = getgenv().ShiftlockEnabled
				and "rbxassetid://101910992099753"
				or "rbxassetid://80450981243325"
		end)
	end

	if v then
		if getgenv().ShiftlockConn then return end

		getgenv().ShiftlockConn = RunService.RenderStepped:Connect(function()
			if not getgenv().ShiftlockEnabled then return end
			humanoid.AutoRotate = false
			hrp.CFrame = CFrame.new(hrp.Position, hrp.Position + camera.CFrame.LookVector)
		end)
	else
		getgenv().ShiftlockEnabled = false
		if getgenv().ShiftlockConn then
			pcall(function() getgenv().ShiftlockConn:Disconnect() end)
			getgenv().ShiftlockConn = nil
		end
		if humanoid then
			humanoid.AutoRotate = true
		end
		if getgenv().ShiftlockButton then
			getgenv().ShiftlockButton.Image = "rbxassetid://80450981243325"
		end
	end
end
})

OthersTab:Input({
Title = "TTPI",
Desc = "Teleport To Game Using The Place Id",
Value = "",
InputIcon = "link",
Type = "Input",
Placeholder = "PLACE ID",
Callback = function(input)
local id = tonumber(input) or tonumber(string.match(input, "%d+"))
getgenv().TargetPlaceId = id
end
})

OthersTab:Button({
Title = "Teleport",
Callback = function()
if getgenv().TargetPlaceId then
WindUI:Notify({
Title = "TTPI NOTIFICATION",
Content = "TELEPORTING...",
Duration = 5,
Icon = "target",
})
task.wait(5)
game:GetService("TeleportService"):Teleport(
getgenv().TargetPlaceId,
game:GetService("Players").LocalPlayer
)
end
end
})

