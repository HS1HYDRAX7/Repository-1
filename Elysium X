FPESTab:Toggle({
	Title="Fake Miss Circle Enrage Mode",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState = {} end
		local state = CallbackState

		local Players = game:GetService("Players")
		local lp = Players.LocalPlayer

		if state.vfx then state.vfx:Destroy() state.vfx = nil end
		if state.sound1 then state.sound1:Stop() state.sound1:Destroy() state.sound1 = nil end
		if state.sound2 then state.sound2:Stop() state.sound2:Destroy() state.sound2 = nil end

		if not v then return end

		local vfx = Instance.new("ColorCorrectionEffect")
		vfx.TintColor = Color3.fromRGB(255,50,50)
		vfx.Contrast = 0.2
		vfx.Saturation = -0.5
		vfx.Parent = workspace.CurrentCamera
		state.vfx = vfx

		local sound1 = Instance.new("Sound", workspace)
		sound1.SoundId = "rbxassetid://17548871120"
		sound1.Volume = 1
		sound1:Play()
		state.sound1 = sound1

		local sound2 = Instance.new("Sound", workspace)
		sound2.SoundId = "rbxassetid://17548873081"
		sound2.Volume = 1
		state.sound2 = sound2

		task.delay(2, function()
			if state.sound2 then
				state.sound2:Play()
			end
		end)

		WindUI:Notify({
			Title="Miss Circle",
			Content="You failed",
			Duration=2,
			Icon="rbxassetid://17224686449"
		})
	end
})

FPESTab:Toggle({
	Title="Fake Blackout",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState = {} end
		local state = CallbackState

		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
		local lp = Players.LocalPlayer
		local camera = workspace.CurrentCamera

		if state.vfx then state.vfx:Destroy() state.vfx = nil end
		if state.gui then state.gui:Destroy() state.gui = nil end
		if state.conn then state.conn:Disconnect() state.conn = nil end
		if state.sound1 then state.sound1:Stop() state.sound1:Destroy() state.sound1 = nil end
		if state.sound2 then state.sound2:Stop() state.sound2:Destroy() state.sound2 = nil end

		if not v then return end

		local vfx = Instance.new("ColorCorrectionEffect")
		vfx.TintColor = Color3.fromRGB(255,255,255)
		vfx.Contrast = 0.25
		vfx.Saturation = -1
		vfx.Brightness = 0.03
		vfx.Parent = camera
		state.vfx = vfx

		local gui = Instance.new("ScreenGui")
		gui.IgnoreGuiInset = true
		gui.ResetOnSpawn = false
		gui.Parent = lp:WaitForChild("PlayerGui")
		state.gui = gui

		local scan = Instance.new("ImageLabel")
		scan.Size = UDim2.fromScale(1,1)
		scan.BackgroundTransparency = 1
		scan.Image = "rbxassetid://1316045217"
		scan.ImageTransparency = 0.88
		scan.ScaleType = Enum.ScaleType.Tile
		scan.TileSize = UDim2.new(1,0,0,6)
		scan.Parent = gui

		state.conn = RunService.RenderStepped:Connect(function()
			scan.Position = UDim2.fromOffset(0, math.random(-2,2))
		end)

		local sound1 = Instance.new("Sound", workspace)
		sound1.SoundId = "rbxassetid://133703811807029"
		sound1.Volume = 1
		sound1:Play()
		state.sound1 = sound1

		local sound2 = Instance.new("Sound", workspace)
		sound2.SoundId = "rbxassetid://116634437254886"
		sound2.Volume = 1
		sound2.Looped = true
		state.sound2 = sound2

		task.delay(2, function()
			if state.sound2 then
				state.sound2:Play()
			end
		end)
	end
})

FPESTab:Toggle({
	Title="Proximity Alert",
	Value=false,
	Callback=function(v)
		if not CallbackState then CallbackState = {} end
		local state = CallbackState
		if state.conn then state.conn:Disconnect() state.conn = nil end
		if not v then return end

		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
		local lp = Players.LocalPlayer
		local last = 0

		state.conn = RunService.Heartbeat:Connect(function()
			local char = lp.Character
			local hrp = char and char:FindFirstChild("HumanoidRootPart")
			if not hrp then return end

			local function checkFolder(folder)
				if not folder then return end
				if char.Parent == folder then return end

				for _,m in ipairs(folder:GetChildren()) do
					if m:IsA("Model") then
						local root = m:FindFirstChild("HumanoidRootPart")
						if root and (root.Position - hrp.Position).Magnitude <= 15 then
							if os.clock() - last >= 3 then
								last = os.clock()
								WindUI:Notify({
									Title="Proximity Alert",
									Content = m.Name .. " is too close!",
									Duration = 3,
									Icon = "rbxassetid://10653372143",
								})
							end
							return true
						end
					end
				end
			end

			if checkFolder(workspace:FindFirstChild("Teachers")) then return end
			checkFolder(workspace:FindFirstChild("Alices"))
		end)
	end
})

BWGTab:Toggle({
	Title="ESP Monsters",
	Value=false,
	Callback=function(v)
		local MonstersList = {
			SkinStealer=true,
			Smiler=true,
			Hound=true,
			PartyGoer=true,
			Insanity=true
		}

		if not _G.__ESP_MONSTERS then
			_G.__ESP_MONSTERS = {highlights={}, conns={}}
		end
		local state = _G.__ESP_MONSTERS

		for _,h in ipairs(state.highlights) do if h then h:Destroy() end end
		for _,c in ipairs(state.conns) do if c then c:Disconnect() end end
		state.highlights = {}
		state.conns = {}

		if not v then return end

		local function apply(obj)
			if MonstersList[obj.Name] then
				local h = Instance.new("Highlight")
				h.Adornee = obj
				h.FillColor = Color3.fromRGB(152,6,6)
				h.OutlineColor = Color3.fromRGB(132,5,5)
				h.FillTransparency = 0.5
				h.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
				h.Parent = obj
				table.insert(state.highlights, h)
			end
		end

		for _,obj in ipairs(workspace:GetChildren()) do
			apply(obj)
		end

		table.insert(state.conns, workspace.ChildAdded:Connect(apply))
	end
})

BWGTab:Toggle({
	Title="Aimbot Monsters",
	Value=false,
	Callback=function(v)
		local RunService = game:GetService("RunService")
		local Players = game:GetService("Players")
		local Camera = workspace.CurrentCamera
		local LocalPlayer = Players.LocalPlayer
		local Smoothness = 0.12
		local MonstersList = {
			SkinStealer=true,
			Smiler=true,
			Hound=true,
			PartyGoer=true,
			Insanity=true
		}

		if _G.__AIMBOT_MONSTERS then
			_G.__AIMBOT_MONSTERS:Disconnect()
			_G.__AIMBOT_MONSTERS = nil
		end

		if not v then return end

		local rayParams = RaycastParams.new()
		rayParams.FilterType = Enum.RaycastFilterType.Blacklist

		_G.__AIMBOT_MONSTERS = RunService.RenderStepped:Connect(function()
			local character = LocalPlayer.Character
			if character then
				rayParams.FilterDescendantsInstances = {character}
			end

			local closest
			local dist = math.huge
			local camPos = Camera.CFrame.Position

			for _,obj in ipairs(workspace:GetChildren()) do
				if MonstersList[obj.Name] then
					local part = obj:FindFirstChildWhichIsA("BasePart", true)
					if part then
						local dir = part.Position - camPos
						local result = workspace:Raycast(camPos, dir, rayParams)
						if result and result.Instance:IsDescendantOf(obj) then
							local d = dir.Magnitude
							if d < dist then
								dist = d
								closest = part
							end
						end
					end
				end
			end

			if closest then
				local target = CFrame.new(camPos, closest.Position)
				Camera.CFrame = Camera.CFrame:Lerp(target, Smoothness)
			end
		end)
	end
})

BWGTab:Button({
	Title="Weapon Shop",
	Callback=function()
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer
		local Character = LocalPlayer.Character
		if not Character then return end
		local HRP = Character:FindFirstChild("HumanoidRootPart")
		if not HRP then return end

		local store = workspace:FindFirstChild("StoreOwner")
		if not store then return end

		local part = store:IsA("BasePart") and store or store:FindFirstChildWhichIsA("BasePart", true)
		if not part then return end

		local cf = part.CFrame
		HRP.CFrame = CFrame.new(cf.Position + cf.LookVector * 5, cf.Position)
	end
})

